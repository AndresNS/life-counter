{
  "expo": {
    "name": "life-counter",
    "slug": "life-counter",
    "version": "0.3.0",
    "orientation": "portrait",
    "scheme": "life-counter",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "androidStatusBar": {
      "hidden": true
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.andresns.lifecounter"
    },
    "web": {
      "favicon": "./src/assets/favicon.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "7e224b3d-efa4-4a73-b99f-905c9d4c7f0c"
      }
    },
    "plugins": ["expo-router"]
  }
}
